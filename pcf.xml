<playercorefactory>
	<players>
		<player name="VLC" type="ExternalPlayer" audio="false" video="true">
			<filename>org.videolan.vlc</filename>
			<hidexbmc>false</hidexbmc>
		</player>
		<player name="MX" type="ExternalPlayer" audio="false" video="true">
			<filename>com.mxtech.videoplayer.ad</filename>
			<hidexbmc>false</hidexbmc>
		</player>
		<player name="AceStream" type="ExternalPlayer" audio="false" video="true">
			<filename>org.acestream.media</filename>
			<hidexbmc>false</hidexbmc>
		</player>
		<player name="AceEngine" type="ExternalPlayer" audio="false" video="true">
			<filename>org.acestream.core</filename>
			<hidexbmc>false</hidexbmc>
		</player>
		<player name="AceStreamTV" type="ExternalPlayer" audio="false" video="true">
			<filename>org.acestream.media.atv</filename>
			<hidexbmc>false</hidexbmc>
		</player>
		<player name="AceEngineTV" type="ExternalPlayer" audio="false" video="true">
			<filename>org.acestream.core.atv</filename>
			<hidexbmc>false</hidexbmc>
		</player>
		<player name="Wiseplay" type="ExternalPlayer" audio="false" video="true">
			<filename>com.wiseplay</filename>
			<hidexbmc>false</hidexbmc>
		</player>
		<player name="WuffyPlayer" type="ExternalPlayer" audio="false" video="true">
			<filename>co.wuffy.player</filename>
			<hidexbmc>false</hidexbmc>
		</player>
		<player name="Downloader" type="ExternalPlayer" audio="false" video="true">
			<filename>downloader.vitmate.downloaderapp</filename>
			<hidexbmc>false</hidexbmc>
		</player>
		<player name="WebVideoCaster" type="ExternalPlayer" audio="false" video="true">
			<filename>com.instantbits.cast.webvideo</filename>
			<hidexbmc>false</hidexbmc>
		</player>
		<player name="VideoDownloader" type="ExternalPlayer" audio="false" video="true">
			<filename>video.downloader.videodownloader</filename>
			<hidexbmc>true</hidexbmc>
		</player>
		<player name="Chrome" type="ExternalPlayer" audio="false" video="true">
			<filename>com.android.chrome</filename>
			<hidexbmc>false</hidexbmc>
		</player>
	</players>
	
	<rules action="overwrite">
		<rule name="acestream" filename="acestream://.*" player="AceStream" />
		<rule name="wise" filename=".*extPL=wise" player="Wiseplay" />
		<rule name="other" video="true" player="videodefaultplayer" />
	</rules>
		
</playercorefactory>
